<%- include('_layout/header') %>
<meta name="viewport" content="width=device-width, initial-scale=1">
<style>
  p {
    margin-left: 5%;
    margin-right: 5%;
    font-size: 40px;
    color: black;
    font-weight: 700;
    height: 15vh;
    width: 90%;
    font-family: "Times New Roman", Times, serif;
    margin-bottom: 4px;
  }
  .animation {
    width: 70vw;
    height: 60vh;
    position: relative;
    animation-name: example;
    animation-duration: 6s;
    animation-delay: 2s;
  }
  /* .animation .responsive {
  width: 100%;
  max-width: 400px;
  height: auto;
} */

  @keyframes example {
    0% {
      left: 0px;
      top: 0px;
    }
    25% {
      left: 200px;
      top: 0px;
    }
    50% {
      left: 200px;
      top: 200px;
    }
    75% {
      left: 0px;
      top: 200px;
    }
    100% {
      left: 0px;
      top: 0px;
    }
  }
</style>

<p>Welcome to CMS of Greenwich</p>
<div class="animation">
  <img
    src="https://www.gre.ac.uk/__data/assets/image/0025/119653/gre.jpg"
    alt="Paris"
    width="100%"
    height="100%"
    class="responsive"
  />
</div>

<%- include('_layout/footer') %>
